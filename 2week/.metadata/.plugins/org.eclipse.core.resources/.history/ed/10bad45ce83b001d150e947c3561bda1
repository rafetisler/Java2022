public class loopDemo {

	public static void main(String[] args) {
		
		// For
		for(int i=6; i<=10;i++) {
			System.out.println(i);
				}
		System.out.println("For Döngüsü Bitti");
					
		//While
		int i=1;
		while (i<10) {
			System.out.println(i);
			i++;		
			
		}
		System.out.println("While Döngüsü bitti");
		
		// Do-While
		do {
			
		} while (i<10);
		System.out.println(i);
		i++;	
			
	}System.out.println("do-While Döngüsü bitti");

}
